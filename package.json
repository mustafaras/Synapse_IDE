{
  "name": "coder-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "dev:trace": "vite --open '/?trace=1'",
    "dev:safe": "vite --host 0.0.0.0 --port 3000",
    "dev:cmd": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "vitest run",
    "test:ci": "vitest run --reporter=junit --outputFile=junit.xml",
    "e2e": "playwright test",
    "e2e:ci": "playwright test --reporter=line",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test:dev": "vite --open http://localhost:5173?tests=1&autorun=1",
    "test:preview": "vite preview --open http://localhost:4173?tests=1&autorun=1",
    "preview": "vite preview",
    "clean": "rimraf dist",
    "release:changelog": "node scripts/release/changelog.cjs",
    "release:version": "node scripts/release/bump.cjs",
    "release:tag": "git tag v$(node -p \"require('./package.json').version\") && git push --tags",
    "release": "npm run release:version && npm run release:changelog && npm run build",
    "build:eval": "tsc -p tsconfig.eval.json",
    "eval": "npm run build:eval && node scripts/eval/run.cjs",
    "eval:record": "npm run build:eval && node scripts/eval/record.cjs",
    "eval:ci": "npm run build:eval && node scripts/eval/run.cjs src/eval/datasets/examples/basic.yaml eval_reports"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-query-devtools": "^5.80.7",
    "@types/jszip": "^3.4.0",
    "@types/prismjs": "^1.26.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@types/three": "^0.177.0",
    "framer-motion": "^12.18.1",
    "immer": "^10.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.515.0",
    "monaco-editor": "^0.52.2",
    "openai": "^5.3.0",
    "pdf-lib": "^1.17.1",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "styled-components": "^6.1.19",
    "three": "^0.177.0",
    "use-immer": "^0.11.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.0.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "conventional-changelog": "^7.0.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.0",
    "semver": "^7.6.0",
    "tailwindcss": "^4.1.10",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^2.1.9"
  }
}
